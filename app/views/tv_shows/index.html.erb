<h3>Tv Shows</h3>
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Channel Name</th>
      <th scope="col">Start Time</th>
      <th scope="col">End Time</th>
    </tr>
  </thead>
  <tbody>
    <% @tv_shows.each_with_index do |tv_show, index| %>
      <tr>
        <th scope="row"><%= index + 1 %></th>
        <td><%= tv_show.name %></td>
        <td><%= tv_show.channel_name %></td>
        <td><%= tv_show.start_time %></td>
        <td><%= tv_show.end_time %></td>
      <tr>
    <% end %>
  </tbody>
</table>